algoritmo "Aulas de VisuALG"

// Comandos usados: Enquanto,Atribui��o de Valores e Multiplica��o

var
X,CONTADOR:inteiro
inicio

escreval("Qual tabuada deseja calcular?")
leia(X)
CONTADOR<-1
enquanto (CONTADOR<=10) faca
   escreval(X," x",CONTADOR,"=",X*CONTADOR)
   CONTADOR<-CONTADOR+1
fimenquanto
fimalgoritmo
