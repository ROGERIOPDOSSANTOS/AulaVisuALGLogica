algoritmo "Aulas de VisuALG"

var
SALARIO:real
NOME:caractere
HORAS:inteiro
inicio

escreval("Qual o nome do instrutor?")
leia(NOME)
escreval("Quantas horas ele trabalha por dia?")
leia(HORAS)
escreval("Quanto ele ganha por hora?")
leia(SALARIO)
escreval("O instrutor",NOME,"recebe:R$",HORAS*SALARIO,", por dia.")


fimalgoritmo
